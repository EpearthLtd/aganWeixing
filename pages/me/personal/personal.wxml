<view class="weui-cells">
  <view class="weui-cell" wx:if="{{personalInfo[0]!=null}}">
    <view class="weui-cell__bd">姓名</view>
    <view class="weui-cell__ft">{{personalInfo[0]}}</view>
  </view>
  <view class="weui-cell" wx:else>
    <view class="weui-cell__bd">姓名</view>
    <view class="weui-cell__ft">未报名</view>
  </view>
  <view class="weui-cell" wx:if="{{personalInfo[1]!=null}}">
    <view class="weui-cell__bd">学校</view>
    <view class="weui-cell__ft">{{personalInfo[1]}}</view>
  </view>
  <view class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">手机号</view>
    <view class="weui-cell__ft weui-cell__ft_in-access">{{personalInfo[2]}}</view>
  </view>
</view>
